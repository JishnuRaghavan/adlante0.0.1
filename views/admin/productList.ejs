  <!-- sa-app__toolbar / end --><!-- sa-app__body -->
  <div id="app-content">
    <!-- Container fluid -->
    <div class="app-content-area">
      <div class="container-fluid">
        <div class="row">
          <div class="col-lg-12 col-md-12 col-12">
            <!-- Page header -->
            <div class="mb-5">
              <h3 class="mb-0 ">Products</h3>
            </div>
          </div>
        </div>
        <div>
          <!-- row -->
          <div class="row">
            <div class="col-12">
              <!-- card -->
              <div class="card mb-4">
                <div class="card">
                  <div class="sa-divider"></div>
                  <table class="sa-datatables-init" data-order="[[ 1, &quot;asc&quot; ]]" data-sa-search-input="#table-search">
                    <thead>
                      <tr>
                        <th class="min-w-20x">Product</th>
                        <th>Category</th>
                        <th>Stock</th>
                        <th>Price</th>
                        <th class="w-min" data-orderable="false"></th>
                      </tr>
                    </thead>
                    <tbody>
                      <% if(typeof products !== 'undefined') { %>
                        <% products.forEach((product)=>{ %>
                      <tr>
                        <td>
                          <div class="d-flex align-items-center">
                            <a href="app-product.html" class="me-4">
                              <div class="sa-symbol sa-symbol--shape--rounded sa-symbol--size--lg"><img src="productimg/<%=product.defaultimage %>" width="40" height="40" alt=""/></div>
                            </a>
                          </div>
                        </td>
                        <td><a href="app-category.html" class="text-reset"><%= product.category %></a></td>
                        <td>
                          <div class="badge badge-sa-success"><%= product.stock %></div>
                        </td>
                        <td>
                          <div class="sa-price"><span class="sa-price__symbol">$</span><span class="sa-price__integer"><%= product.saleprice %></span>
                        </td>
                        <td>
                          <div class="dropdown">
                            <button class="btn btn-sa-muted btn-sm" type="button" id="product-context-menu-0" data-bs-toggle="dropdown" aria-expanded="false" aria-label="More">
                              <svg xmlns="http://www.w3.org/2000/svg" width="3" height="13" fill="currentColor">
                                <path d="M1.5,8C0.7,8,0,7.3,0,6.5S0.7,5,1.5,5S3,5.7,3,6.5S2.3,8,1.5,8z M1.5,3C0.7,3,0,2.3,0,1.5S0.7,0,1.5,0 S3,0.7,3,1.5S2.3,3,1.5,3z M1.5,10C2.3,10,3,10.7,3,11.5S2.3,13,1.5,13S0,12.3,0,11.5S0.7,10,1.5,10z"></path>
                              </svg>
                            </button>
                            <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="product-context-menu-0">
                              <form action="/admin/editProduct" method="post">
                                <input type="hidden" value="<%= product._id %>" name="productId">
                                <li><button type="submit" class="dropdown-item" style="border: none;background: transparent;">Edit</button></li>
                                <li>
                                  <hr class="dropdown-divider"/>
                                </li>
                                <li><a class="dropdown-item text-danger" href="#">Delete</a></li>
                              </form>
                            </ul>
                          </div>
                        </td>
                      </tr>
                      <% }) %>
                      <% } %>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</main>
