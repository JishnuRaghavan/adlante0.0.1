<% if(typeof categories !== 'undefined') { %>
  <div id="app-content">
    <form action="/admin/addCategoryPage" method="post">
      <!-- sa-app__toolbar / end --><!-- sa-app__body -->
      <div id="top" class="app-content-area">
        <div class="mx-sm-2 px-2 px-sm-3 px-xxl-4 pb-6">
          <div class="container container--max--xl">
            <div class="py-5">
              <div class="row g-4 align-items-center">
                <div class="col">
                  <h1 class="h3 m-0">Add Category</h1>
                </div>
                <div class="col-auto d-flex"><button type="submit" class="btn btn-primary">Save</button></div>
              </div>
            </div>
            <div class="sa-entity-layout" data-sa-container-query="{&quot;920&quot;:&quot;sa-entity-layout--size--md&quot;,&quot;1100&quot;:&quot;sa-entity-layout--size--lg&quot;}">
              <div class="sa-entity-layout__body">
                <div class="sa-entity-layout__main">
                  <div class="card">
                    <div class="card-body p-5">
                      <div class="mb-5">
                        <h2 class="mb-0 fs-exact-18">Basic information</h2>
                      </div>
                      <div class="mb-4">
                        <label for="form-category/name" class="form-label">Name</label>
                        <input id="categoryName" type="text" class="form-control" name="maincategory" value="Men's wear"/>
                        <div id="categoryMessage" style="color: red;"><%= categoryMessage %></div>
                      </div>
                      <div class="mb-4">
                        <label for="form-category/description" class="form-label">Description</label><textarea name="description" class="sa-quill-control form-control" rows="8">Lorem ipsum dolor sit amet, consectetur adipiscing elit. Curabitur ornare, mi in ornare elementum, libero nibh lacinia urna, quis convallis lorem erat at purus. Maecenas eu varius nisi.</textarea>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="sa-entity-layout__sidebar">
                  <div class="card w-100">
                    <div class="card-body p-5">
                      <div class="mb-5">
                        <h2 class="mb-0 fs-exact-18">Visibility</h2>
                      </div>
                      <div class="mb-4">
                        <label class="form-check">
                          <input type="radio" class="form-check-input" value="visible" name="visibility"/>
                          <span class="form-check-label">Visible</span>
                        </label>
                        <label class="form-check mb-0">
                          <input type="radio" class="form-check-input" value="hidden" name="visibility"/><span class="form-check-label">Hidden</span>
                        </label>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </form>
  </div>
  <% } %>
